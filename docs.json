{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Polychads API",
  "colors": {
    "primary": "#8b5cf6",
    "light": "#a78bfa",
    "dark": "#7c3aed"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "authentication",
              "quickstart"
            ]
          },
          {
            "group": "Real-Time",
            "pages": [
              "real-time/websocket",
              "real-time/webhooks"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "reference/alert-object",
              "reference/signal-score",
              "reference/categories",
              "reference/tiers",
              "reference/errors",
              "reference/rate-limits"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/overview"
            ]
          },
          {
            "group": "Alerts",
            "pages": [
              "api-reference/alerts/get-latest",
              "api-reference/alerts/get-alerts",
              "api-reference/alerts/get-alert-by-id"
            ]
          },
          {
            "group": "Account",
            "pages": [
              "api-reference/stats",
              "api-reference/me"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://polychadsbot.xyz",
          "icon": "chart-mixed"
        },
        {
          "anchor": "Swagger",
          "href": "https://polychadsbot.xyz/docs",
          "icon": "rectangle-terminal"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://polychadsbot.xyz"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://t.me/polychads"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },
  "footer": {
    "socials": {
      "telegram": "https://t.me/polychads"
    }
  }
